<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE env PUBLIC "-//GOYY//DTD ENV 1.0//EN" "http://goyy.org/dtd/env-1.0.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<upload name="goyy">
				<dir>/assets/yyupl</dir>
				<url>/upls</url>
				<maxSize>5242880</maxSize>
			</upload>
		</environment>
		<environment id="test">
			<upload name="goyy">
				<dir>/assets/yyupl</dir>
				<url>http://127.0.0.1:9099/yyupl</url>
				<maxSize>5242880</maxSize>
			</upload>
		</environment>
		<environment id="production">
			<upload name="goyy">
				<dir>/assets/yyupl</dir>
				<url>http://assets.goyy.org/yyupl</url>
				<maxSize>5242880</maxSize>
			</upload>
		</environment>
	</environments>
</configuration>
